{
	"manifest_version":2,

	"name":"getting started example",
	"background":{
		"scripts":["eventPage.js"],
		"persistent":false
	},
	"content_scripts":{
		"matches":["http:*"],
		"css":["mystyle.css"],
		"js":["jquery.js","myscript.js"]
	},
	"description":"this extension show a Google Image serach research for the current page",
	"version":"1.0",

	"browser_action":{
		"default_icon":"icon.png",
		"default_popup":"popup.html",
		"default_title":"click here!"

	},

	"permissions":[
		"activeTab",
		"https://pro-core-148422.appspot.com/count/*"
	]
}